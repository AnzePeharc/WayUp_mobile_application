<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- WayUp regular font -->
    <font
        android:font="@font/flamante_round_medium_ffp"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/flamante_round_medium_ffp"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- Open Sans regular -->
    <font
        android:font="@font/opensans_regular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/opensans_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- Open Sans italic -->
    <font
        android:font="@font/opensans_italic"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/opensans_italic"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- Open Sans bold -->
    <font
        android:font="@font/opensans_bold"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/opensans_bold"
        app:fontStyle="normal"
        app:fontWeight="400" />


</font-family>